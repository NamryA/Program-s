import java.util.Scanner;

class Main {
  public static void main(String[] args){
   Scanner scanner = new Scanner(System.in);
    int soma = 0;

    System.out.println("Numeros de 1 a 100:");

    for (int i = 1; i <= 100; i++){
      System.out.printIn(i);
      soma += i;
      } 
    System.out.println("A soma dos numeros Ã©: " + soma);
    
  scanner.close();
  }
}
